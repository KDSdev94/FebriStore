{
  "expo": {
    "name": "Febri Store",
    "slug": "febri-store",
    "version": "2.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#4A90E2"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.febri.store",
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "Aplikasi ini memerlukan akses lokasi untuk fitur alamat pengiriman otomatis.",
        "NSLocationAlwaysAndWhenInUseUsageDescription": "Aplikasi ini memerlukan akses lokasi untuk fitur alamat pengiriman otomatis."
      }
    },
    "android": {
      "icon": "./assets/icon.png",
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#4A90E2"
      },
      "package": "com.febri.store",
      "versionCode": 2,
      "googleServicesFile": "./google-services.json",
      "permissions": [
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.RECORD_AUDIO"
      ]
    },
    "web": {
      "favicon": "./assets/splash-icon.png"
    },
    "plugins": [
      "expo-image-picker",
      "expo-document-picker",
      "expo-location",
      "expo-maps"
    ],
    "extra": {
      "eas": {
        "projectId": "fc488935-8e71-4cb0-9b55-fd446090247a"
      }
    },
    "owner": "kecapmanis1"
  }
}
